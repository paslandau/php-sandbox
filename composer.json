{
  "name": "paslandau/php-sandbox",
  "license": "MIT",
  "description": "A sandbox-like implementation for executing PHP code with restriction to certain language constructs (functions etc.)",
  "authors": [
    {
      "name": "Pascal Landau",
      "email": "kontakt@myseosolution.de",
      "homepage": "http://www.myseosolution.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "paslandau\\PhpSandbox\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.myseosolution.de/"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.5",
    "nikic/php-parser": "^1.4.1"
  },
  "require-dev": {
    "paslandau/io-utility": "dev-master",
    "phpunit/phpunit": "~4"
  },
  "config": {
    "secure-http": false
  }
}