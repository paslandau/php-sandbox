{
  "name": "paslandau/php-sandbox",
  "license": "MIT",
  "description": "A sandbox-like implementation for executing PHP code with restriction to certain language constructs (functions etc.)",
  "authors": [
    {
      "name": "Pascal Landau",
      "email": "kontakt@myseosolution.de",
      "homepage": "http://www.myseosolution.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
